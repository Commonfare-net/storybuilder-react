{
  "name": "storybuilder-react",
  "version": "0.1.0",
  "description": "Story builder for Commonfare.net platform",
  "main": "build/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack --watch",
    "build": "webpack -p",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Aaron Ciaghi",
  "babel": {
    "presets": [
      "env"
